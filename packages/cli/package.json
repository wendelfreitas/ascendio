{
  "name": "ascendio",
  "version": "0.0.0",
  "description": "An CLI designed to simplify the initial setup of your turborepo project. Ideal for indie developers or people looking to create a micro SaaS.",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": "./dist/index.js",
  "files": [
    "dist",
    "template",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "package.json"
  ],
  "keywords": [
    "ascendio",
    "init.tips",
    "storybook",
    "jest",
    "next.js",
    "tailwind",
    "typescript",
    "shadcn",
    "ui"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "start": "node dist/index.js",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "lint": "eslint src --max-warnings 0",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "changeset version",
    "pub:release": "pnpm build && pnpm publish --no-git-checks --access public",
    "pub:beta": "pnpm build && pnpm publish --no-git-checks --access public --tag beta"
  },
  "devDependencies": {
    "@ascendio/eslint-config": "workspace:*",
    "@ascendio/typescript-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.6",
    "eslint": "^8.57.0",
    "tsup": "^8.0.1",
    "type-fest": "^4.10.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "ora": "^8.0.1",
    "prompts": "^2.4.2",
    "replace-in-file": "^8.0.1"
  }
}
